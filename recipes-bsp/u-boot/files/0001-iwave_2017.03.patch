+.macro imx6_ddr_setting
+	ldr r0, =ANATOP_BASE_ADDR
+	ldr r1, [r0,#0x260]
+	ldr r3,=0xff0000
+	and r1,r1,r3
+	cmp r1,#0x630000
+	beq quad_cpu
+
+	ldr r0, =OCOTP_BASE_ADDR
+	ldr r1, [r0,#0x430]
+	ldr r3,=0x100000
+	and r1,r1,r3
+	cmp r1,#0x100000
+	beq solo_cpu	
+	cmp r1,#0x0
+	beq duallite_cpu
+
