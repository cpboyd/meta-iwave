+	imx6solo_iwg15m_ddr_setting
+	b common_ddr
+
+duallite_cpu:
+	imx6dl_iwg15m_ddr_setting
+	b common_ddr
+
+quad_cpu:
+	imx6dq_iwg15m_ddr_setting
+	b common_ddr
+
