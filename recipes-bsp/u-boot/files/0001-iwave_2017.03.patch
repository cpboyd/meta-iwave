+int board_late_init(void)
+{
+#ifdef CONFIG_CMD_BMODE
+	add_board_boot_modes(board_boot_modes);
+#endif
+
+#ifdef CONFIG_LDO_BYPASS_CHECK
+        /*Set the LDO voltages 
+          pass a dummy integer value as function requires
+          pass the arguments for ldo_bypass
+          0 - ldo active mode
+          1 - ldo bypass mode
+         */
+        ldo_mode_set(check_uboot_ldobypass());
+#endif
+
+   print_board_info ();
+
+	return 0;
+}
+
+int checkboard(void)
+{
+        u32 cpurev;
+
+        cpurev = get_cpu_rev();
+        if(is_mx6dqp())
+                printf("Board: i.MX%sP iW-RAINBOW-G15M-SM\n",get_imx_type((cpurev & 0xFF000) >> 12));
+        else
+                printf("Board: i.MX%s iW-RAINBOW-G15M-SM\n",get_imx_type((cpurev & 0xFF000) >> 12));
+
+        printf("Boot Device: ");
+        switch (get_boot_device()) {
+                case SATA_BOOT:
+                        printf("SATA\n");
+                        break;
+                case SPI_NOR_BOOT:
+                        printf("SPI NOR\n");
+                        break;
+                case SD1_BOOT:
+                        printf("STANDARD SD\n");
+                        break;
+                case SD3_BOOT:
+                        printf("MICRO SD\n");
+                        break;
+                case MMC4_BOOT:
+                        printf("MMC\n");
+                        break;
+                case UNKNOWN_BOOT:
+                default:
+                        printf("UNKNOWN\n");
+                        break;
+        }
+
+	return 0;
+}
