+			vpu@02040000 {
+				status = "okay";
+			};
+
+			iomuxc: iomuxc@020e0000 {
+				compatible = "fsl,imx6q-iomuxc";
+			};
+		};
+
+		aips-bus@02100000 { /* AIPS2 */
