+static void imx6_iwg15_flexcan1_switch_auto(int enable)
+{
+	/* Active low enables the CAN tranceiver */
+	if (enable)
+		gpio_set_value_cansleep(flexcan1_en_gpio, 0);
+	else
+		gpio_set_value_cansleep(flexcan1_en_gpio, 1);
