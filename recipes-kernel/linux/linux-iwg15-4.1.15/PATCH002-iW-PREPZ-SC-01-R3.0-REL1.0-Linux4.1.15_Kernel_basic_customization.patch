+        pinctrl-names = "default";
+        pinctrl-0 = <&pinctrl_pwm3_4>;
+        status = "okay";
+};
+&pwm4 {
+        pinctrl-names = "default";
+        pinctrl-0 = <&pinctrl_pwm4_4>;
+        status = "okay";
+};
